<?xml version="1.0"?>
<launch>

    <!-- Run the map server -->
    <!-- <arg name="map_file" default="$(find jackal_navigation)/maps/jackal_race.yaml"/> -->
    <arg name="map_file" default="/home/linsc/ROS_Project/Sensetime_ws/dengwenjun_old_ws/src/rtk_gps/map/test_whole.yaml"/>
    <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

    <!--- Run AMCL -->
    <!-- <include file="$(find simple_coverage_global_planner)/launch/include/amcl.launch" /> -->

    <node name="static_map_to_odom_publisher" pkg="tf" type="static_transform_publisher" args="0 0 0 0 0 0 /map /odom 100"/>

    <!--- Run Move Base -->
